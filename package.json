    "typescript": "^5.7.0",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/config": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
